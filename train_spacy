python3.6 -m spacy train en models/spacy/onto_cased data/spacy_ready/cased/train.json data/spacy_ready/cased/dev.json --n-iter 500 --n-early-stopping 3 --pipeline ner --verbose
python3.6 -m spacy train en models/spacy/onto_uncased data/spacy_ready/uncased/train.json data/spacy_ready/uncased/dev.json --n-iter 500 --n-early-stopping 3 --pipeline ner --verbose
